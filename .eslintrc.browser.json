{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "commonjs": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "env": true,
    "sinon": true,
    "deepFreeze": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-use-before-define": 0,
    "comma-dangle": 0,
    "one-var": 0,
    "padded-blocks": 0,
    "indent": [
      2,
      4
    ]
  }
}